<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kingslayer.fosmis">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
        <activity android:name=".NoticeActivity" />
        <activity android:name=".CombinationRegistration" />
        <activity android:name=".EditSubmittedCombinations" />
        <activity android:name=".CourseRegistration" />
        <activity
            android:name=".OptionalCourseRegistration"
            android:parentActivityName=".CourseRegistration"></activity>
        <activity android:name=".ExamRegistration"></activity>
    </application>

</manifest>